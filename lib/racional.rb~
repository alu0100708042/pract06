
# creando getters  para comprobar la prueba


class Racional
	attr_reader :num, :denom
	def initialize (num,denom)
	# se llama al maximo comun divisor para reducir la funcion dividiendo entre ese numero
		reducir = gcd(num, denom)
		@num = num/reducir
		@denom = denom/reducir
	end
	
	def to_s
		"#{num}/#{denom}"
	end
end
